<addon id="plugin.program.communitywizard" name="[COLOR darkorange][B]The Community Wizard[/B][/COLOR]" version="10.1.7" provider-name="@communityrepo">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">[COLOR orange]The Community Wizard[/COLOR]</summary>
    <description lang="en">All the latest Community Builds. Contact The Team on Twitter @CommunityRepo to add your build</description>
    <platform>all</platform>
  </extension>
</addon>


	
